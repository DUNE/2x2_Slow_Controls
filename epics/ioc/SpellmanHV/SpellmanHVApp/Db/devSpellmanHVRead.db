record(ai, "spellmanhv/rVSet"){
  field(DESC, "Set voltage")
  field(DTYP, "stream")
  field(INP, "@devSpellmanHV.proto get_set_i $(PORT) $(BUS)")
  field(EGU, "kV")
  field(SCAN, "2 second")
  field(LINR,"SLOPE")
  field(PREC, "2")
  field(AOFF,"0.00")
  field(ASLO, "0.01221")
}

